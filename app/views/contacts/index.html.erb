
<% if user2_signed_in? %>
<table class="table table-striped table-bordered table-hover">
  <thead class="thead-dark">
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <% @contacts.each do |contact| %>
    <tr>
      <td> 
         <%= link_to contact.name, contact%>
      </td>
       <td> <%=  contact.phone%></td>
      <td> <%=  contact.email%></td>
      </tr>
      <%end%>
    </tbody>
  </table>
  <br>

  <%= link_to "New contact", new_contact_path ,class:"btn btn-secondary"%>

    


  <%else%>
