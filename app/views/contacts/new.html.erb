<h1> Add New Contact</h1>

<%= form_with model: @contact do |form| %>
  <div>
    <%= form.label :PhoneNumber %><br>
    <%= form.text_field :PhoneNumber %>
    <% @contact.errors.full_messages_for(:PhoneNumber).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

  <div>
    <%= form.label :Name %><br>
    <%= form.text_field :Name %><br>
    <% @contact.errors.full_messages_for(:Name).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>
  

  <div>
    <%= form.label :Email %><br>
    <%= form.text_field :email %><br>
    <% @contact.errors.full_messages_for(:email).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>




  <div>
    <%= form.submit %>
  </div>
<% end %>